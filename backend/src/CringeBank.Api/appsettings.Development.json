{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "Telemetry": {
    "Seq": {
      "Url": "http://localhost:5341",
      "ApiKey": "",
      "MinimumLevel": "Debug"
    },
    "Tracing": {
      "Exporter": "console",
      "ServiceVersion": "dev"
    },
    "Metrics": {
      "Exporter": "prometheus",
      "ScrapeEndpointPath": "/metrics"
    }
  },
  "Workers": {
    "FirebaseUserSynchronization": {
      "IntervalSeconds": 180,
      "PageSize": 200,
      "MaxUsersPerIteration": 1000,
      "StartupDelaySeconds": 10
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "http://localhost:5273"
    ]
  },
  "Swagger": {
    "Enabled": true
  }
}
