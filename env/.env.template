# CringeBank Ortam Degiskeni Sabloonu
# Her bir satiri kopyalarken gercek degerleri girin ve bu dosyayi versiyon kontrolune eklemeyin.

# Genel Kimlik/Senkron ayarlari
CRINGEBANK_API_BASE=https://api.example.com
CRINGEBANK_OUTBOX_RETRY_WINDOW=PT5M

# Firebase emülatörleri icin
CRINGEBANK_USE_FIREBASE_EMULATOR=false
FIREBASE_AUTH_EMULATOR_HOST=localhost:9099
FIRESTORE_EMULATOR_HOST=localhost:8787
FIREBASE_STORAGE_EMULATOR_HOST=localhost:9199

# Firebase remote testleri icin opsiyonel kimlik bilgileri
CRINGEBANK_RUN_FIREBASE_REMOTE_TESTS=false
CRINGEBANK_REMOTE_EMAIL=
CRINGEBANK_REMOTE_PASSWORD=

# Test/gelistirme telemetri secenekleri
CRINGEBANK_TELEMETRY_ENDPOINT=https://telemetry.example.com/collect
CRINGEBANK_TELEMETRY_API_KEY=

# App Check / reCAPTCHA gibi guvenlik anahtarlari
CRINGEBANK_APPCHECK_DEBUG_TOKEN=
CRINGEBANK_RECAPTCHA_PUBLIC_KEY=
CRINGEBANK_RECAPTCHA_SECRET_KEY=

# Sentry veya diger hata izleme servisleri
SENTRY_DSN=
SENTRY_ENV=local
